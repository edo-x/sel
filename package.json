{
  "name": "@edo-x/sel",
  "version": "0.0.0",
  "description": "SEL (SQL expression language) is a typescript library for building SQL statements programmatically using builder style DSL.",
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js",
    "./*": "./dist/src/*"
  },
  "files": [
    "./dist/src/*"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc && tsc-alias",
    "test": "vitest",
    "cover": "vitest run --coverage",
    "coverv": "start dist/.coverage/index.html",
    "lint": "eslint  --cache --cache-location ./dist .",
    "dev": "f [dev:*]",
    "watch:build": "tsc --watch",
    "watch:alias": "tsc-alias --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "frunk": "^0.0.4",
    "lefthook": "^1.13.6",
    "lint-staged": "^16.2.4",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": ["eslint --fix"]
  }
}
